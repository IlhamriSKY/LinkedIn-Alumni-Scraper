# LinkedIn Alumni Scraper - Frontend Configuration Example
# Copy this file to .env and modify as needed

# Development Server Configuration
VITE_DEV_PORT=5173
VITE_DEV_HOST=localhost

# Production Configuration
VITE_PROD_PORT=3000
VITE_PROD_HOST=localhost

# Backend API Configuration
VITE_BACKEND_PORT=3001
VITE_BACKEND_HOST=localhost
VITE_BACKEND_URL=http://localhost:3001

# API Base URLs
VITE_API_BASE_URL=http://localhost:3001/api
VITE_SOCKET_URL=http://localhost:3001

# Environment
NODE_ENV=development

# Note: Environment variables with VITE_ prefix are available in the browser
# Change these URLs if you're running on different hosts/ports

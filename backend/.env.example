# LinkedIn Alumni Scraper Environment Configuration
# Copy this file to .env and modify the values as needed

# =====================================================
# LINKEDIN CREDENTIALS (REQUIRED)
# =====================================================
LINKEDIN_EMAIL=your_linkedin_email@example.com
LINKEDIN_PASSWORD=your_linkedin_password

# =====================================================
# UNIVERSITY INFORMATION
# =====================================================
UNIVERSITY_NAME=Your University Name
UNIVERSITY_LINKEDIN_ID=your-university-linkedin-id
UNIVERSITY_LINKEDIN_URL=https://www.linkedin.com/school/your-university/people/
UNIVERSITY_LINKEDIN_URL_ALT=https://www.linkedin.com/school/your-university/

# =====================================================
# SERVER CONFIGURATION
# =====================================================
PORT=3001
HOST=localhost
NODE_ENV=development

# =====================================================
# FRONTEND CONFIGURATION
# =====================================================
FRONTEND_DEV_PORT=5173
FRONTEND_PROD_PORT=3000
FRONTEND_HOST=localhost
FRONTEND_DEV_URL=http://localhost:5173
FRONTEND_PROD_URL=http://localhost:3000

# =====================================================
# BROWSER SETTINGS
# =====================================================
HEADLESS=false
DISABLE_WEB_SECURITY=false
BLOCK_IMAGES=false
BLOCK_CSS=false

# =====================================================
# TIMING CONFIGURATION (milliseconds)
# =====================================================
# Default delays for general operations
DEFAULT_DELAY_MIN=2000
DEFAULT_DELAY_MAX=5000

# Operation-specific delays - humanize automation
DELAY_SEARCH=3000
DELAY_NAVIGATION=2000
DELAY_PROFILE=4000
DELAY_TYPING=800
DELAY_CLICK=1500

# Browser timeouts
LOGIN_TIMEOUT=20000
NAVIGATION_TIMEOUT=30000
ELEMENT_WAIT_TIMEOUT=15000
SEARCH_TIMEOUT=10000
PROFILE_LOAD_TIMEOUT=15000
BROWSER_TIMEOUT=30000
WAIT_TIMEOUT=5000

# Retry configuration
MAX_RETRIES=3

# Rate limiting
REQUESTS_PER_MINUTE=30
CONCURRENT_REQUESTS=3

# =====================================================
# FILE PATHS
# =====================================================
SEARCH_NAMES_FILE=../search_names.csv
RESULTS_DIRECTORY=./results
LOGS_DIRECTORY=./logs

# =====================================================
# SECURITY
# =====================================================
USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"

# =====================================================
# ANTI-DETECTION SETTINGS
# =====================================================
ENABLE_STEALTH=true
VIEWPORT_WIDTH=1366
VIEWPORT_HEIGHT=768
HUMAN_LIKE_DELAYS=true
RANDOM_MOUSE_MOVEMENTS=true

# =====================================================
# DEBUG SETTINGS
# =====================================================
DEBUG_MODE=false
SAVE_SCREENSHOTS=false
SAVE_PAGE_CONTENT=false
LOG_LEVEL=error
LOG_ACCESS=false
CONSOLE_LOG_LEVEL=minimal
VERBOSE_LOGS=false

# =====================================================
# LINKEDIN CSS SELECTORS - CONFIGURABLE FOR CHANGES
# =====================================================

# Login Page Selectors
LOGIN_USERNAME_SELECTOR=#username
LOGIN_PASSWORD_SELECTOR=#password
LOGIN_SUBMIT_SELECTOR=button[type="submit"]

# Navigation Selectors
GLOBAL_NAV_SELECTOR=.global-nav, .scaffold-layout__nav, .feed-nav
